#!/bin/sh
#ls the Models folder
echo $pwd
ls ./Models
which larod-client
#for loop for each file in Models print name
for file in ./Models/*
do
    echo $file
    #run larod client and extract text
    #larod-client -R 1000 -p -c axis-a8-dlpu-tflite -g $file -i '' | grep "Mean execution time for job:"
    larod-client -R 1000 -p -c axis-a8-dlpu-tflite -g $file -i '' | grep "Mean execution time for job:"
done
